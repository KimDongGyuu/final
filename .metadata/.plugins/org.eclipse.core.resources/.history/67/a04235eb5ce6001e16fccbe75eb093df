<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ace.company_event">
<insert id="company_eventWrite">
insert into company_event 
values(#{com_idx},company_event_event_idx.nextval,#{event_title},#{event_content},sysdate,0,ADD_MONTHS(SYSDATE, 2),0,0,0,#{event_addr},#{event_writer})
</insert>

<select id="company_eventList" parameterType="Integer" resultType="com.ace.company_event.model.Company_eventDTO">
select * from company_event where com_idx=#{com_idx} order by event_idx desc
</select>
</mapper>
